            android:theme="@style/AppTheme.TranslucentStatus" />
        <activity android:name=".testcase.annotation.AnnotationActivity" />
        <activity android:name=".testcase.filedownload.DownloadActivity" />

        <service android:name=".testcase.filedownload.DownloadService" />

