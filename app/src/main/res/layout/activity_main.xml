<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.master.kit.TestActivity.MainActivity"
    >


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
      >

        <Button
            android:id="@+id/CalendarActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="CalendarActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/PageLayoutActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="PageLayoutActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/GesturePasswordActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="GesturePasswordActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/TouchActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="TouchActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/ViewPagerActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="ViewPagerActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/DesignDispatchActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="DesignDispatchActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/ListViewActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="ListViewActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/SoftKeyboardActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="SoftKeyboardActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/CPBActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="CPBActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/WebViewActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="WebViewActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/CameraActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="CameraActivity"
            android:textAllCaps="false" />


    </LinearLayout>
</ScrollView>