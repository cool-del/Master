<?xml version="1.0" encoding="utf-8"?>
<com.ooftf.vertical.ScrollEdgeInterceptorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/interceptor"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical"
    app:scrollId="@+id/recycler_view">
    <ooftf.com.widget.self.ReturnTopLayout xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:scrollId="@+id/recycler_view">
        <android.support.v7.widget.RecyclerView
            android:id="@+id/recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <include layout="@layout/layout_sticky_header" />
    </ooftf.com.widget.self.ReturnTopLayout>
</com.ooftf.vertical.ScrollEdgeInterceptorLayout>