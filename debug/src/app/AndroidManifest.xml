<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ooftf.master.debug">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name="com.ooftf.service.base.MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.BackgroundTransparent"
        tools:replace="android:allowBackup,android:icon, android:theme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.NewInstanceActivity" />
        <activity
            android:name=".activity.KeyBoardActivity"
            android:theme="@style/AppTheme.TranslucentStatus" />
        <activity android:name=".activity.DrawableActivity" />
        <activity android:name=".activity.FingerprintActivity" />
        <activity android:name=".activity.VerticalPagerActivity" />
        <activity android:name=".activity.TranslationActivity" />
        <activity android:name=".activity.RegexActivity" />
        <activity android:name=".activity.DialogActivity" />
        <activity android:name=".activity.ThreadPoolActivity" />
        <activity android:name=".activity.HashMapActivity" />
        <activity android:name=".activity.ViewModelActivity" />
        <activity android:name=".activity.AcrossRecyclerActivity" />
        <activity android:name=".activity.AnnotationActivity" />
        <activity android:name=".activity.ConstraintLayoutActivity" />
        <activity android:name=".activity.LayoutManagerActivity" />
    </application>

</manifest>